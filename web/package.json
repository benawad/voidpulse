{
  "name": "@voidpulse/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_API_URL=http://localhost:4001 next dev --turbopack",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "pages:build": "pnpm dlx @cloudflare/next-on-pages@1",
    "deploy": "pnpm run pages:build && wrangler pages deploy --branch=prod .vercel/output/static",
    "lint": "next lint",
    "themes": "ts-node --project tsconfig.scripts.json scripts/convertHexThemes.ts",
    "svgr": "npx @svgr/cli --typescript --no-index --icon --out-dir src/app/svgx -- svgs"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.8",
    "@formatjs/intl-numberformat": "^8.10.0",
    "@hello-pangea/dnd": "^16.5.0",
    "@juggle/resize-observer": "^3.4.0",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-table": "^8.11.8",
    "@tanstack/react-virtual": "^3.0.4",
    "@trpc/client": "11.0.0-next-beta.242",
    "@trpc/next": "11.0.0-next-beta.242",
    "@trpc/react-query": "11.0.0-next-beta.242",
    "@trpc/server": "11.0.0-next-beta.242",
    "@types/react-dates": "^21.8.6",
    "@voidpulse/api": "workspace:*",
    "@types/react-modal": "^3.16.3",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "downshift": "^8.3.1",
    "emoji-picker-react": "^4.7.11",
    "moment": "^2.30.1",
    "next": "15.3.3",
    "react": "19.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-dates": "^21.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.49.3",
    "react-icons": "^5.0.1",
    "react-modal": "^3.16.1",
    "react-toastify": "^10.0.4",
    "tailwindcss-animate": "^1.0.7",
    "use-resize-observer": "^9.1.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.9.0",
    "@cloudflare/workers-types": "^4.20240222.0",
    "@svgr/cli": "^8.1.0",
    "@types/css": "^0.0.37",
    "@types/node": "^20.11.10",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "css": "^3.0.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wrangler": "^3.29.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.6",
      "@types/react-dom": "19.1.5"
    }
  }
}
